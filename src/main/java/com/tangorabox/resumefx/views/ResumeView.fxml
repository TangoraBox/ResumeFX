<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.*?>
<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="800.0" prefWidth="1200.0"
         type="BorderPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tangorabox.resumefx.views.ResumeView">
    <right>
        <StackPane alignment="TOP_CENTER" maxWidth="450.0" minWidth="0.0" BorderPane.alignment="TOP_CENTER">
            <children>
                <VBox fx:id="rightPane" alignment="TOP_CENTER"/>
                <VBox fx:id="aboutPane" alignment="BOTTOM_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                      minHeight="-Infinity" minWidth="-Infinity" prefHeight="280.0"
                      StackPane.alignment="BOTTOM_CENTER"/>
            </children>
        </StackPane>
    </right>
    <center>
        <VBox fx:id="centerPane" alignment="TOP_CENTER" BorderPane.alignment="TOP_CENTER"/>
    </center>
    <bottom>
        <HBox alignment="CENTER_LEFT" styleClass="bottom-bar" BorderPane.alignment="CENTER">
            <children>
                <Label styleClass="title1" text="ResumeFX"/>
                <HBox alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <children>
                        <ToggleButton fx:id="projectInfo" mnemonicParsing="false" text="Project Info"/>
                    </children>
                </HBox>
                  </children>
              </HBox>
          </bottom>
</fx:root>
